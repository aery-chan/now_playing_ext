{
	"manifest_version": 2,
	"name": "Now Playing",
	"version": "0.1",

	"permissions": [
		"tabs"
	],

	"background": {
		"scripts": [ "build/build.js" ]
	},
	
	"browser_action": {
		"default_icon": {
			"64": "img/disconnected.png"
		},
		"default_popup": "action/index.html",
		"default_title": "Disconnected"
	}
}